{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/fetch-configs.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.ts' --runInBand",
    "test:integration:watch": "jest --watch --testMatch='**/__tests__/integration/**/*.test.ts' --runInBand",
    "test:quick": "jest --testMatch='**/__tests__/integration/bulk-agent-test.test.ts' --runInBand",
    "test:extended": "jest --testMatch='**/__tests__/integration/extended-test-suite.test.ts' --runInBand",
    "test:comprehensive": "jest --testMatch='**/__tests__/integration/{bulk-agent-test,extended-test-suite}.test.ts' --runInBand",
    "test:unit": "jest --testMatch='**/__tests__/**/*.test.ts' --testPathIgnorePatterns='/integration/' --runInBand",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:integration && npm run test:e2e",
    "test:report": "ts-node --project tsconfig.node.json scripts/generate-bug-report.ts",
    "test:report:quick": "npm run test:quick -- --json --outputFile=test-results/jest-results.json && ts-node --project tsconfig.node.json scripts/generate-bug-report.ts",
    "test:report:extended": "npm run test:extended -- --json --outputFile=test-results/jest-results.json && ts-node --project tsconfig.node.json scripts/generate-bug-report.ts",
    "test:report:comprehensive": "npm run test:comprehensive -- --json --outputFile=test-results/jest-results.json && ts-node --project tsconfig.node.json scripts/generate-bug-report.ts",
    "test:live:quick": "npm run test:quick -- --verbose",
    "test:live:extended": "npm run test:extended -- --verbose",
    "test:live:comprehensive": "npm run test:comprehensive -- --verbose",
    "playwright:install": "npx playwright install",
    "postinstall": "prisma generate && prisma migrate deploy",
    "db:reset": "node reset-database.js --force",
    "db:studio": "prisma studio",
    "db:check-scores": "node check-user-scores.js",
    "db:merge-sessions": "node merge-duplicate-sessions.js",
    "admin:create": "ts-node --project tsconfig.node.json scripts/create-admin.ts"
  },
  "dependencies": {
    "@coinbase/cdp-sdk": "^1.38.5",
    "@coinbase/x402": "^0.7.1",
    "@coral-xyz/anchor": "^0.32.1",
    "@prisma/client": "^6.19.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.95.8",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "lucide-react": "^0.468.0",
    "next": "^15.0.0",
    "node-cache": "^5.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "x402": "latest",
    "x402-next": "latest"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.709.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "bs58": "^5.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
