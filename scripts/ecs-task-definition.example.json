{
  "family": "pardon-simulator",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "2048",
  "memory": "4096",
  "executionRoleArn": "arn:aws:iam::YOUR_ACCOUNT_ID:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::YOUR_ACCOUNT_ID:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "coral-server",
      "image": "YOUR_ACCOUNT_ID.dkr.ecr.us-east-1.amazonaws.com/coral-server:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5555,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/pardon-simulator",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "coral-server"
        }
      }
    },
    {
      "name": "agent-cz",
      "image": "YOUR_ACCOUNT_ID.dkr.ecr.us-east-1.amazonaws.com/pardon-agent:latest",
      "essential": false,
      "command": ["/app/fetch-and-start.sh", "cz"],
      "workingDirectory": "/app/cz",
      "environment": [
        {"name": "AGENT_NAME", "value": "cz"},
        {"name": "CORAL_AGENT_ID", "value": "cz"},
        {"name": "CORAL_SSE_URL", "value": "http://localhost:5555/sse/v1/devmode/app/priv/production-main"},
        {"name": "MODEL_API_KEY", "value": "YOUR_OPENAI_API_KEY_HERE"},
        {"name": "MODEL_NAME", "value": "gpt-5.1"},
        {"name": "MODEL_PROVIDER", "value": "openai"},
        {"name": "MODEL_TEMPERATURE", "value": "0.7"},
        {"name": "MODEL_MAX_TOKENS", "value": "8000"},
        {"name": "TIMEOUT_MS", "value": "300"},
        {"name": "SOLANA_RPC_URL", "value": "https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY"},
        {"name": "SOLANA_PRIVATE_KEY", "value": "YOUR_CZ_SOLANA_PRIVATE_KEY_BASE58"},
        {"name": "AWS_REGION", "value": "us-east-1"}
      ],
      "dependsOn": [
        {
          "containerName": "coral-server",
          "condition": "START"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/pardon-simulator",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "agent-cz"
        }
      }
    },
